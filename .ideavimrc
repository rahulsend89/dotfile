" Selected IntelliSpace modules
source ~/intelli-space/spacemacs.vim
source ~/intelli-space/extra.vim
source ~/intelli-space/major.vim
source ~/intelli-space/hybrid.vim

" My own vim commands
nnoremap Y y$
let mapleader=","
set easymotion
set multiple-cursors                  "<A+c> <A+x> <A+a>
set hlsearch
set ignorecase
set incsearch
set number
set relativenumber
set showmode
set smartcase
set visualbell
set wrapscan
set clipboard
set gdefault
set surround
map ,xc :noh<CR>
map <S-J> 5j
map <S-K> 5k
map <S-H> ^
map <S-L> $
map vw vaw
map cw caw
